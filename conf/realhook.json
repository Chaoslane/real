{
  "realhook": {
    "host": "192.168.4.153",
    "stat_port": "18079",
    "stat_path": "/cp_status",
    "websocket_port": "18080"
  },
  "servers": [
    {
      "host": "192.168.4.2",
      "port": "8080",
      "path": "/realhook/set_checkpoint"
    }
  ],
  "redis" : {
    "host" : "192.168.4.2",
    "port" : "6379"
  },
  "tcpserver" : {
    "host" : "192.168.4.5",
    "port" : "6789"
  },
  "cprules": [
    {
      "name": "login",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=(auto|click)Login)"
        }
      ]
    },
    {
      "name": "llzk",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=CB_(GXBDG|QTTHBDG|RBDG|RXDG|XSBDG))"
        }
      ]
    },
    {
      "name": "rwk",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=CB_app)"
        }
      ]
    },
    {
      "name": "zdcx",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=2002)"
        }
      ]
    },
    {
      "name": "xdcx",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=2003)"
        }
      ]
    },
    {
      "name": "llcx",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=2012)"
        }
      ]
    },
    {
      "name": "4gfx",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_s=4GFXTC_[0-9]{2}[A-B]{0,1}_TC|si_n=4GLLK)"
        }
      ]
    },
    {
      "name": "4gzx",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_s=4GFXTC_[0-9]{2,3}_[0-9]{2,3}_TC)"
        }
      ]
    },
    {
      "name": "4gll",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_s=4GLLB)"
        }
      ]
    },
    {
      "name": "cxl",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=(2001|2002|2003|2004|2012|JiFen-QN))"
        }
      ]
    },
    {
      "name": "bll",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=(?!(2001|2002|2003|2004|2012|JiFen-QN|autoLogin|clickLogin|YJDL|yueLogin|SY_DL_FW|SY_DL_DX)))"
        }
      ]
    },
    {
      "name": "mobile",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:mobile=[0-9]{11})"
        }
      ]
    },
    {
      "name": "all",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": ".*"
        }
      ]
    }
  ]
}

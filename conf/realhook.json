{
  "realhook": {
    "host": "192.168.4.153",
    "stat_port": "18089",
    "stat_path": "/cp_status",
    "websocket_port": "18090"
  },
  "servers": [
    {
      "host": "192.168.4.2",
      "port": "8080",
      "path": "/realhook/set_checkpoint"
    }
  ],
  "redis" : {
    "host" : "192.168.4.2",
    "port" : "6379"
  },
  "tcpserver" : {
    "host" : "192.168.4.5",
    "port" : "6789"
  },
  "cprules": [
    {
      "name": "login",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=YJDL|si_n=SY_DL|si_n=DL)"
        }
      ]
    },
    {
      "name": "llzk",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=CB_GXBDG|si_n=CB_QTTHBDG|si_n=CB_RBDG|si_n=CB_RXDG|si_n=CB_XSBDG)"
        }
      ]
    },
    {
      "name": "rwk",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=CB_app)"
        }
      ]
    },
    {
      "name": "zdcx",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=2002)"
        }
      ]
    },
    {
      "name": "xdcx",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=2003)"
        }
      ]
    },
    {
      "name": "llcx",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=2012)"
        }
      ]
    },
    {
      "name": "4gfx",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_s=4GFXTC_[0-9]{2}[A-B]{0,1}_TC|si_n=4GLLK)"
        }
      ]
    },
    {
      "name": "4gzx",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_s=4GFXTC_[0-9]{2,3}_[0-9]{2,3}_TC)"
        }
      ]
    },
    {
      "name": "4gll",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_s=4GLLB)"
        }
      ]
    },
    {
      "name": "cxl",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:si_n=2001|si_n=2002|si_n=2003|si_n=2004|si_n=2012|si_n=JiFen-QN)"
        }
      ]
    },
    {
      "name": "bll",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?!si_n=2001|si_n=2002|si_n=2003|si_n=2004|si_n=2012|si_n=JiFen-QN)"
        }
      ]
    },
    {
      "name": "mobile",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": "(?:mobile=[0-9]{11})"
        }
      ]
    },
    {
      "name": "all",
      "rule": [
        {
          "check": "$cs_uri_query",
          "pcre": ".*"
        }
      ]
    }
  ]
}
